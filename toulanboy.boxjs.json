{
    "id": "toulanboy.app.sub",
    "name": "toulanboy 脚本订阅",
    "author": "@toulanboy",
    "icon": "https://avatars0.githubusercontent.com/u/12446619?s=460&v=4",
    "repo": "https://github.com/toulanboy/scripts",
    "apps": [
      {
        "id": "weather-simple",
        "name": "下雨提醒",
        "keys": ["tlb_weather_url"],
        "settings": [
            { "id": "tlb_rain_test", "name": "脚本测试", "val": false, "type": "boolean", "desc": "" },
            { "id": "tlb_always_notify", "name": "所有情况都弹出通知（默认是降雨概率大于50%才弹）", "val": false, "type": "boolean", "desc": "默认是降雨概率大于50%才弹出通知" },
            { "id": "tlb_weather_url", "name": "天气链接", "val": "", "type": "text", "placeholder":"这里填天气链接", "desc": "打开https://weather.com/zh-CN/weather/today， 搜索你的城市，然后切换到【每小时】。复制地址栏的链接填到这"},
            { "id": "tlb_pre_hours", "name": "预测未来的小时数", "val": 24, "type": "text", "desc": "最大只能填48" },
            { "id": "tlb_rain_timeout", "name": "[debug]请求超时限制 (毫秒)", "val": 2000, "type": "text", "desc": "" }
        ],
        "author": "@toulanboy",
        "repo": "https://github.com/toulanboy/scripts/blob/master/weather",
        "icons": ["https://raw.githubusercontent.com/Orz-3/mini/master/weather.png", "https://raw.githubusercontent.com/Orz-3/task/master/weather.png"]
      },
      {
        "id": "weibotalk",
        "name": "微博超话",
        "keys": ["evil_tokenurl", "evil_tokencheckinurl", "evil_tokenheaders", "evil_tokencheckinheaders","evil_tokenurl2", "evil_tokencheckinurl2", "evil_tokenheaders2", "evil_tokencheckinheaders2"],
        "settings": [
            { "id": "wb_delete_cookie", "name": "打开清空cookie功能", "val": false, "type": "boolean", "desc": "清空cookie功能开关" },
            { "id": "wb_msg_max_num", "name": "每个通知显示超话数", "val": 30, "type": "text", "desc": "每页最显示多少个超话" },
            { "id": "wb_request_time", "name": "签到间隔 (毫秒)", "val": 700, "type": "text", "desc": "每个超话签到的间隔（超话大于50个的，建议填1000以上）" }
        ],
        "author": "@toulanboy, @evilbutcher",
        "repo": "https://github.com/toulanboy/scripts/blob/master/weibo",
        "icons": ["https://raw.githubusercontent.com/Orz-3/mini/master/weibo.png", "https://raw.githubusercontent.com/Orz-3/task/master/weibo.png"]
      }
    ]
  }